{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "/request-context/request-context-admin-user.json",
  "title": "RequestContextAdminUser",
  "description": "The admin user making the request. Extensions do not see this user in the Users table.",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "permissions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["id", "email", "permissions"]
}
